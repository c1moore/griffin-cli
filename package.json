{
  "name": "griffin-cli",
  "version": "0.1.3",
  "description": "Tool for pulling secrets and other config with version control.",
  "author": "Calvin Moore @c1moore",
  "bin": {
    "griffin": "./bin/run"
  },
  "homepage": "https://github.com/griffin-cli/griffin-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "griffin-cli/griffin-cli",
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/LICENSE",
    "/README.md"
  ],
  "dependencies": {
    "@aws-sdk/client-ssm": "^3.438.0",
    "@oclif/core": "^3",
    "@oclif/errors": "^1.3.6",
    "@oclif/plugin-autocomplete": "^3.0.1",
    "@oclif/plugin-help": "^6.0.5",
    "@oclif/plugin-update": "^4.1.3",
    "@oclif/plugin-warn-if-update-available": "^3.0.2",
    "csv-stringify": "^6.4.4",
    "dotenv": "^16.3.1",
    "envfile": "^6.18.0",
    "yaml": "^2.3.3"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.433.0",
    "@oclif/test": "^3.0.4",
    "@types/chai": "^4",
    "@types/chai-as-promised": "^7.1.7",
    "@types/mocha": "^10.0.3",
    "@types/mock-fs": "^4.13.3",
    "@types/node": "^20.8.9",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "aws-sdk-client-mock": "^3.0.0",
    "chai": "^4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.52.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "mocha": "^10",
    "mock-fs": "^5.2.0",
    "mock-stdin": "^1.0.0",
    "nyc": "^15.1.0",
    "oclif": "^4.0.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "oclif": {
    "bin": "griffin",
    "dirname": "griffin",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-update"
    ],
    "topicSeparator": " ",
    "topics": {
      "ssm": {
        "description": "Manage SSM-based config."
      },
      "exec": {
        "description": "Execute a command, injecting config as environment variables."
      },
      "export": {
        "description": "Export config in the format specified."
      }
    }
  },
  "scripts": {
    "clean": "shx rm -rf dist && shx rm -f tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "prepublishOnly": "npm prune --omit dev",
    "test": "GRIFFIN_AWS_SSM_ENDPOINT=http://localhost:4566 nyc mocha \"test/**/*.test.ts\"",
    "test:unit": "mocha \"test/**/*.test.ts\" --exclude \"test/integration/*.test.ts\"",
    "test:integration": "GRIFFIN_AWS_SSM_ENDPOINT=http://localhost:4566 mocha \"test/integration/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "up": "docker compose up -d",
    "down": "docker compose down --volumes"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "bugs": "https://github.com/griffin-cli/griffin-cli/issues",
  "keywords": [
    "oclif",
    "secrets",
    "config",
    "ssm"
  ],
  "types": "dist/index.d.ts"
}
